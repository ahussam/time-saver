{
  "manifest_version": 2,
  "name": "TimeSaver",
  "description": "A chrome extension to redirect you from social media sites to useful resource URLs.",
  "version": "0.1",
  "permissions": [
    "tabs"
],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "block.png"
  }
}